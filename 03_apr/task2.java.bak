class task2 
{
	public static void main(String[] args) 
	{
		System.out.println("program start");
		int start=100,end=1;
		while(start>=end){
			if(start%2==0){
				System.out.println(start+:"java");
			}
			start--;
			else{

			if(start%2!=0){
				System.out.println(end);

			}
			start--;
			else{
						System.out.println("program end");

			}
		}
	}
}
}
